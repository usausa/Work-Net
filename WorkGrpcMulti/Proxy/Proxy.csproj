<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Grpc.AspNetCore" Version="2.67.0" />
    <PackageReference Include="Grpc.Net.Client" Version="2.67.0" />
  </ItemGroup>

  <ItemGroup>
    <Protobuf Include="..\Proto\ClientProxy.proto" GrpcServices="Server" Link="Proto\ClientProxy.proto" />
    <Protobuf Include="..\Proto\ProxyServer.proto" GrpcServices="Client" Link="Proto\ProxyServer.proto" />
  </ItemGroup>

</Project>
